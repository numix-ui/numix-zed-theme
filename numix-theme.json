      "settings": {
        "foreground": "#e57373"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#e57373"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffb74d"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "support.constant", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#ffb74d"
      }
    },
    {
