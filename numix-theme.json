  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9a9a9a"
      }
    },
    {
      "name": "String - Docstring",
      "scope": ["string.quoted.docstring", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#81c784"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#ffcdd2",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#ba68c8"
      }
    },
    {
      "name": "Storage Modifier",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#ba68c8"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "storage.type.annotation"],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#d0d0d0"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#e57373"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
